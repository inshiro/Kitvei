<?xml version="1.0" encoding="utf-8"?>
<resources>



    <!-- style -->
    <declare-styleable name="ThemeColor">
        <attr name="root_color" format="reference|color" />
        <attr name="line_color" format="reference|color" />
        <attr name="title_color" format="reference|color" />
        <attr name="subtitle_color" format="reference|color" />
        <attr name="content_color" format="reference|color" />
    </declare-styleable>

    <!-- StatusBarView -->
    <declare-styleable name="StatusBarView">
        <attr name="sbv_translucent_mode" format="boolean" />
        <attr name="sbv_fill_in_mode" format="boolean" />
        <attr name="sbv_init_alpha" format="float" />
        <attr name="sbv_darker_alpha" format="float" />
    </declare-styleable>

    <!-- circleImageView -->
    <declare-styleable name="CircleImageView">
        <attr name="civ_border_width" format="dimension" />
        <attr name="civ_border_color" format="color" />
        <attr name="civ_border_overlay" format="boolean" />
        <attr name="civ_fill_color" format="color" />
    </declare-styleable>

    <!-- FreedomImageView -->
    <declare-styleable name="FreedomImageView">
        <attr name="fiv_not_free" format="boolean" />
        <attr name="fiv_cover_mode" format="boolean" />
        <attr name="fiv_shadow_position">
            <enum name="top" value="1" />
            <enum name="bottom" value="-1" />
            <enum name="both" value="2" />
            <enum name="none" value="0" />
        </attr>
    </declare-styleable>

    <!-- FreedomTouchView -->
    <declare-styleable name="FreedomTouchView">
        <attr name="ftv_cover_mode" format="boolean" />
    </declare-styleable>

    <!-- SwipeBackCoordinatorLayout -->
    <declare-styleable name="SwipeBackCoordinatorLayout">
        <attr name="sbl_force_back" format="boolean" />
    </declare-styleable>

    <!-- RippleButton -->
    <declare-styleable name="RippleButton">
        <attr name="rb_title_on" format="string" />
        <attr name="rb_title_off" format="string" />
    </declare-styleable>

    <!-- LiftOff -->
    <declare-styleable name="LiftOff">
        <attr name="initElevation" format="dimension" />
        <attr name="finalElevation" format="dimension" />
    </declare-styleable>

    <!-- CoverMaskLayout -->
    <declare-styleable name="CoverMaskLayout">
        <attr name="cml_gradient_angle" format="float" />
        <attr name="cml_from_alpha" format="float" />
        <attr name="cml_to_alpha" format="float" />
        <attr name="cml_mask_color" format="color" />
    </declare-styleable>

    <!-- Wiki App -->

    <attr name="main_toolbar_color" format="reference"/>
    <attr name="main_status_bar_color" format="reference"/>
    <attr name="page_status_bar_color" format="reference"/>
    <attr name="page_expanded_status_bar_color" format="reference"/>
    <attr name="page_toolbar_color" format="reference"/>
    <attr name="page_toolbar_icon_color" format="reference"/>
    <attr name="toc_item_background" format="reference"/>
    <attr name="edit_improve_tag_selected_drawable" format="reference" />
    <attr name="edit_improve_tag_unselected_drawable" format="reference" />
    <attr name="lead_image_drawable" format="reference"/>
    <attr name="list_separator_drawable" format="reference"/>
    <attr name="list_separator_drawable_bottom_content" format="reference"/>
    <attr name="nearby_marker_drawable" format="reference"/>
    <attr name="tab_item_selector" format="reference"/>
    <attr name="tab_shadow_drawable" format="reference"/>
    <attr name="material_theme_shadow" format="reference"/>
    <attr name="nav_tab_background_color" format="reference" />
    <attr name="nav_tab_item_color_state" format="reference" />
    <attr name="button_highlight_border_shape" format="reference" />

    <attr name="multi_select_background_color" format="reference" />
    <attr name="searchItemBackground" format="reference" />

    <attr name="textColorError" format="color" />
    <attr name="material_theme_primary_color" format="color" />
    <attr name="material_theme_secondary_color" format="color" />
    <attr name="material_theme_de_emphasised_color" format="color" />
    <attr name="material_theme_border_color" format="color" />
    <attr name="primary_text_color" format="color" />
    <attr name="secondary_text_color" format="color" />
    <attr name="section_title_color" format="color" />
    <attr name="paper_color" format="color" />
    <attr name="chart_shade1" format="color" />
    <attr name="chart_shade4" format="color" />
    <attr name="chart_shade5" format="color" />
    <attr name="chart_shade7" format="color" />
    <declare-styleable name="CabSearchView">
        <attr name="cabEnabled" format="boolean" />
    </declare-styleable>

    <declare-styleable name="AutoFitRecyclerView">
        <attr name="minColumnWidth" format="dimension" />
    </declare-styleable>

    <declare-styleable name="EditTextAutoSummarizePreference">
        <attr name="editTextAutoSummarizePreferenceStyle" format="reference" />
        <attr name="autoSummarize" format="boolean" />
    </declare-styleable>

    <declare-styleable name="LongPreference">
        <attr name="longPreferenceStyle" format="reference" />
        <attr name="radix" format="integer" />
        <attr name="summaryFormat" format="string" />
    </declare-styleable>

    <declare-styleable name="IntPreference">
        <attr name="intPreferenceStyle" format="reference" />
    </declare-styleable>

    <declare-styleable name="OnboardingPageView">
        <attr name="centeredImage" format="reference" />
        <attr name="primaryText" format="string" />
        <attr name="secondaryText" format="string" />
        <attr name="tertiaryText" format="string" />
        <attr name="switchText" format="string" />
        <attr name="imageSize" format="dimension" />
        <attr name="background" format="reference" />
        <attr name="showListView" format="boolean" />
        <attr name="dataType" format="string" />
    </declare-styleable>

    <declare-styleable name="DiscreteSeekBar">
        <attr name="min" format="integer" />
        <attr name="tickDrawable" format="reference" />
        <attr name="centerDrawable" format="reference" />
    </declare-styleable>
</resources>